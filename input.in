species     = SO2   					# Species to consider
fileobs     = input/so2_iras4a_red.in			# Input obs file
prefix      = iras4a_so2_red_					# Prefix of output files
choice_y    = jansky   					# Unit of intensity: "kelvin" for main-beam temperatures (moment 0 unit: K km/s) or "jansky" for fluxes (moment 0 unit: Jy km/s)
choice_data = online   					# Where to look for spectro data (local or online)
database    = jpl     					# Spectroscopic database: jpl or cdms
source_lon  = 03h29m10.52        		# Longitude of reference source coordinates
source_lat  = 31d13m31.06        		# Latitude of reference source coordinates
threshold   = 2. 						# Threshold in steps of rms over which transitions are included in the rotational diagram